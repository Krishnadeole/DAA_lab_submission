#include <stdio.h>
#include <string.h>
int main() {
    char a[] = "AGCCCTAAGGGCTACCTAGCTT";
    char b[] = "GACAGCCTACAAGCGTTAGCTTG";
    int m = strlen(a);
    int n = strlen(b);
    int c[100][100];
    char dp[100][100];
    int i, j;
    for (i = 0; i <= m; i++) {
        for (j = 0; j <= n; j++) {
            if (i == 0 || j == 0) {
                c[i][j] = 0;
                dp[i][j] = 'n';
            } else {
                if (a[i - 1] != b[j - 1]) {
                    if (c[i - 1][j] >= c[i][j - 1]) {
                        c[i][j] = c[i - 1][j];
                        dp[i][j] = 'o'; 
                    } else {
                        c[i][j] = c[i][j - 1];
                        dp[i][j] = 's'; 
                    }
                } else {
                    c[i][j] = c[i - 1][j - 1] + 1;
                    dp[i][j] = 'd'; 
                }
            }
        }
    }
    printf("LCS length: %d\n", c[m][n]);
    printf("LCS Matrix:\n");
    for (i = 0; i <= m; i++) {
        for (j = 0; j <= n; j++) {
            printf("%2d ", c[i][j]);
        }
        printf("\n");
    }
    char lcs[100];
    int index = 0;
    i = m;
    j = n;
    while (i > 0 && j > 0) {
        if (dp[i][j] == 'd') {
            lcs[index++] = a[i - 1];
            i--;
            j--;
        } else if (dp[i][j] == 'o') {
            i--;
        } else {
            j--;
        }
    }
    lcs[index] = '\0';
    printf("\nLCS string: ");
    for (int k = index - 1; k >= 0; k--) {
        printf("%c", lcs[k]);
    }
    printf("\n");

    return 0;
}

